    <div class="flex-1 mb-2 sm:mb-0">
        <select class="h-14 bg-zinc-100 text-base font-light rounded-lg border w-full sm:w-62 py-2 px-3" id="stateSelect" name="stateSelect">
            <option value="" disabled selected>Select State</option>
        </select>
    </div>
    <div class="flex-1 mb-2 sm:mb-0">
        <select class="h-14 bg-zinc-100 text-base font-light rounded-lg border w-full sm:w-62 py-2 px-3" id="citySelect" name="citySelect">
            <option value="" disabled selected>Select City</option>
        </select>
    </div>
</div>

<script>
    var stateSelect = document.getElementById('stateSelect');
    var citySelect = document.getElementById('citySelect');

    // Sample data for states and cities
    var states = ["California", "New York", "Texas"];
    var cities = {
        "California": ["Los Angeles", "San Francisco", "San Diego"],
        "New York": ["New York City", "Buffalo", "Albany"],
        "Texas": ["Houston", "Austin", "Dallas"]
    };

    // Populate options for states
    states.forEach(function (state) {
        var optionElement = document.createElement('option');
        optionElement.value = state;
        optionElement.text = state;
        stateSelect.add(optionElement);
    });

    // Populate options for cities based on the selected state
    stateSelect.addEventListener('change', function () {
        var selectedState = stateSelect.value;
        citySelect.innerHTML = ""; // Clear existing options

        if (selectedState in cities) {
            cities[selectedState].forEach(function (city) {
                var optionElement = document.createElement('option');
                optionElement.value = city;
                optionElement.text = city;
                citySelect.add(optionElement);
            });
        }

        // Add a default "Select City" option
        var defaultOption = document.createElement('option');
        defaultOption.value = "";
        defaultOption.text = "Select City";
        citySelect.add(defaultOption);
    });
</script>


